<AnchorPane prefHeight="617.0" prefWidth="1127.0" stylesheets="@../CSS/Login.CSS" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.Login.LoginController">
   <children>
      <ImageView fitHeight="649.0" fitWidth="1132.0" layoutX="-5.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@imagelogin/background.png" />
         </image>
      </ImageView>
      <AnchorPane layoutX="173.0" layoutY="68.0" prefHeight="496.0" prefWidth="776.0">
         <styleClass>
            <String fx:value="a1" />
            <String fx:value="glass-pane" />
         </styleClass>
         <children>
            <ImageView fitHeight="85.0" fitWidth="264.0" layoutX="270.0" layoutY="34.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@imagelogin/logo2024.png" />
               </image>
            </ImageView>
            <ImageView fitHeight="85.0" fitWidth="87.0" layoutX="350.0" layoutY="104.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@imagelogin/logologin.png" />
               </image>
            </ImageView>
            <Button fx:id="loginButton" layoutX="334.0" layoutY="372.0" mnemonicParsing="false" onAction="#login" onKeyPressed="#switchtohomepage" prefHeight="33.0" prefWidth="117.0" text="Login">
               <font>
                  <Font name="#9Slide03 Cabin Bold Bold" size="18.0" />
               </font>
            </Button>
         </children></AnchorPane>
      <PasswordField fx:id="passwordText" layoutX="426.0" layoutY="356.0" prefHeight="36.0" prefWidth="321.0" promptText="password" styleClass="text-field">
         <font>
            <Font size="9.0" />
         </font></PasswordField>
      <ImageView fitHeight="36.0" fitWidth="36.0" layoutX="371.0" layoutY="280.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@imagelogin/usernamelogo.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="54.0" fitWidth="36.0" layoutX="371.0" layoutY="356.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@imagelogin/passwordlogo.png" />
         </image>
      </ImageView>
      <TextField fx:id="usernameText" layoutX="426.0" layoutY="280.0" prefHeight="36.0" prefWidth="321.0" promptText="username" styleClass="text-field" />
      <Label layoutX="461.0" layoutY="516.0" prefHeight="26.0" prefWidth="158.0" text="You don't have account?" textFill="#176b87">
         <font>
            <Font name="Yu Gothic UI Regular" size="14.0" />
         </font></Label>
      <Label fx:id="signuplabel" layoutX="619.0" layoutY="517.0" onMouseClicked="#switchtosignup" prefHeight="25.0" prefWidth="55.0" text="sign up" textFill="#176b87">
         <font>
            <Font name="Verdana Bold Italic" size="12.0" />
         </font></Label>
      <Label fx:id="failtext" layoutX="484.0" layoutY="399.0" styleClass="label-class" textFill="RED">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
